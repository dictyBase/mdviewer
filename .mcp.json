{
	"mcpServers": {
		"playwright": {
			"type": "stdio",
			"command": "bunx",
			"args": [
				"@playwright/mcp@latest",
				"--headless"
			]
		},
		"repomix": {
			"type": "stdio",
			"command": "bunx",
			"args": [
				"repomix@0.3.7",
				"--mcp"
			]
		},
		"memory": {
			"type": "stdio",
			"command": "bunx",
			"args": [
				"@modelcontextprotocol/server-memory"
			]
		},
		"mermaid": {
			"type": "stdio",
			"command": "bunx",
			"args": [
				"@rtuin/mcp-mermaid-validator"
			]
		},
		"context7": {
			"command": "bunx",
			"args": [
				"@upstash/context7-mcp@latest"
			]
		},
		"fetch": {
			"command": "uvx",
			"args": [
				"mcp-server-fetch"
			]
		},
		"github": {
			"type": "stdio",
			"command": "github-mcp-server",
			"args": [
				"stdio"
			]
		},
		"git": {
			"type": "stdio",
			"command": "uvx",
			"args": [
				"mcp-server-git",
				"--repository",
				"/Users/sba964/Projects/devenv/golang/learn-golang/pubmed"
			]
		}
	}
}
